program
    with
        number i;
        number j;
    do
        i = 2;
        j = 15;

        writeln i == 2;       # True
        writeln i /= 2;       # False
        writeln i < 2;        # False
        writeln i > 2;        # False
        writeln i <= 2;       # rue
        writeln i >= 2;       # True
        writeln not (i >= 2); # False
    end;
end;