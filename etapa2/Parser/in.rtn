func circle(number radio)
begin
    arc(360, radio);
end;

func plus(number op1, number op2) -> number
begin
    return op1 + op2;
end;

program
    with
        number i;
    do
        for j from 1.5 to 4.5 by 3 do
            write j, " ";
        end;
    end;

    with
        number i;
    do
        with
            number j = 1;
        do
            while j <= 4 do
                write j, " ";
                j = j + 1;
            end;
        end;
    end;
end;